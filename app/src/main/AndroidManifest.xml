<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="view.com.android_tabview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuickStartActivity"></activity>
        <activity android:name=".CustomInXmlActivity"></activity>
        <activity android:name=".CustomInJavaActivity"></activity>
        <activity android:name=".UseInFragment"></activity>
        <activity android:name=".LoadTabViewActivity"></activity>
    </application>

</manifest>